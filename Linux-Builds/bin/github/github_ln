#!/bin/bash

curDir=$(dirname $0)
. ${curDir}/github_common

[ -z "$1" ] && echo "Miss tag or branch name" && exit 1
TAG_BRANCH_NAME=internal_$1

./build_ce $TAG_BRANCH_NAME

LN_DIR=LN

echo ""
echo "get HPCC LN repo"
[ -d $LN_DIR ] && rm -rf $LN_DIR
LN_REPO=https://github.com/hpcc-systems/LN.git

git clone $LN_REPO 


cd $LN_DIR
get_branch_tag $TAG_BRANCH_NAME
